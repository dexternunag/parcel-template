{
  "name": "parcel-test",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "dev": "concurrently -k \"yarn watch\" \"node browsersync.js\"",
    "start": "parcel src/*.pug --public-url ./",
    "watch": "parcel watch src/*.pug --public-url ./",
    "prebuild": "rm -rf dist",
    "build": "parcel build src/*.pug --public-url ./"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "pug": "^2.0.3",
    "parcel-bundler": "^1.11.0",
    "sass": "^1.16.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.0",
    "bulma": "^0.7.4",
    "browser-sync": "^2.26.3",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7",
    "swiper": "^4.4.6",
    "tether": "^1.4.5"
  }
}
